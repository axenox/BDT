    "contents": {
        "object_alias": "axenox.BDT.run_scenario_status",
        "widget_type": "SplitHorizontal",
        "panels": [
            {
                "caption": "Scenario Statuses",
                "width": "80%",
                "widgets": [
                    {
                        "widget_type": "DataTable",
                        "object_alias": "axenox.BDT.run_scenario",
                        "id": "scenario_table",
                        "row_grouper": {
                            "group_by_attribute_alias": "run_feature__app__LABEL"
                        },
                        "filters": [
                            {
                                "attribute_alias": "run_scenario_status__status",
                                "value": "'failed,paused'"
                            },
                            {
                                "attribute_alias": "run_feature__app"
                            }
                        ],
                        "columns": [
                            {
                                "attribute_alias": "absolute",
                                "hidden": true
                            },
                            {
                                "attribute_alias": "run_feature__filename",
                                "hidden": false
                            },
                            {
                                "attribute_alias": "run_feature__name"
                            },
                            {
                                "attribute_alias": "name",
                                "width_max": "300px"
                            },
                            {
                                "attribute_alias": "run_scenario_status__status"
                            },
                            {
                                "attribute_alias": "run_step__error_message:MAX",
                                "caption": "Error",
                                "width_max": "300px"
                            },
                            {
                                "attribute_alias": "CREATED_ON",
                                "caption": "Last run"
                            },
                            {
                                "attribute_alias": "comment"
                            },
                            {
                                "attribute_alias": "commented_by__USERNAME",
                                "caption": "Commented by"
                            },
                            {
                                "attribute_alias": "tags"
                            },
                            {
                                "attribute_alias": "run_scenario_action__page_alias:LIST_DISTINCT"
                            }
                        ],
                        "sorters": [
                            {
                                "attribute_alias": "run_feature__app__LABEL",
                                "direction": "desc"
                            }
                        ],
                        "buttons": [
                            {
                                "action": {
                                    "alias": "exface.Core.ShowObjectInfoDialog",
                                    "disable_buttons": false
                                },
                                "bind_to_double_click": true
                            },
                            {
                                "caption": "Edit Feature",
                                "action": {
                                    "alias": "exface.Core.ShowObjectEditDialog",
                                    "object_alias": "axenox.BDT.BEHAT_FEATURE",
                                    "input_mapper": {
                                        "from_object_alias": "axenox.BDT.run_scenario",
                                        "to_object_alias": "axenox.BDT.BEHAT_FEATURE",
                                        "column_to_column_mappings": [
                                            {
                                                "from": "run_feature__filename",
                                                "to": "PATHNAME_RELATIVE"
                                            }
                                        ]
                                    }
                                }
                            },
                            {
                                "caption": "Rerun feature",
                                "icon": "repeat",
                                "action": {
                                    "alias": "axenox.BDT.RunTestDialog",
                                    "input_mapper": {
                                        "from_object_alias": "axenox.BDT.run_scenario",
                                        "to_object_alias": "axenox.BDT.run",
                                        "column_to_column_mappings": [
                                            {
                                                "from": "=Concatenate('\"vendor/', run_feature__filename, '\" --tags=Status::Ready')",
                                                "to": "behat_command"
                                            }
                                        ]
                                    }
                                }
                            },
                            {
                                "action_alias": "axenox.BDT.RunScenarioMarkObsolete",
                                "disabled_if": {
                                    "operator": "AND",
                                    "conditions": [
                                        {
                                            "value_left": "=scenario_table!Absolute",
                                            "comparator": "==",
                                            "value_right": 1
                                        }
                                    ]
                                }
                            },
                            {
                                "action_alias": "axenox.BDT.RunScenarioMarkPaused",
                                "disabled_if": {
                                    "operator": "AND",
                                    "conditions": [
                                        {
                                            "value_left": "=scenario_table!paused",
                                            "comparator": "==",
                                            "value_right": 1
                                        }
                                    ]
                                }
                            },
                            {
                                "action_alias": "axenox.BDT.AddRunScenarioComment"
                            }
                        ]
                    }
                ]
            },
            {
                "widgets": [
                    {
                        "widget_type": "SplitVertical",
                        "panels": [
                            {
                                "height": "33%",
                                "widgets": [
                                    {
                                        "caption": "Scenarios tested today",
                                        "widget_type": "Chart",
                                        "object_alias": "axenox.BDT.run_scenario",
                                        "hide_header": true,
                                        "data": {
                                            "aggregate_by_attribute_alias": [
                                                "run_scenario_stats__status"
                                            ],
                                            "filters": [
                                                {
                                                    "attribute_alias": "run_feature__run__finished_on",
                                                    "comparator": ">=",
                                                    "value": "=Today()",
                                                    "hidden": true
                                                }
                                            ]
                                        },
                                        "series": [
                                            {
                                                "type": "pie",
                                                "value_attribute_alias": "UID:COUNT",
                                                "text_attribute_alias": "run_scenario_stats__status"
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "caption": "Apps",
                                "widgets": [
                                    {
                                        "widget_type": "Chart",
                                        "object_alias": "axenox.BDT.run_scenario_status",
                                        "hide_header": true,
                                        "data": {
                                            "aggregate_by_attribute_alias": [
                                                "app_alias",
                                                "status"
                                            ]
                                        },
                                        "series": [
                                            {
                                                "type": "bar",
                                                "caption": "Scenarios",
                                                "x_attribute_alias": "run_scenario:COUNT",
                                                "y_attribute_alias": "app_alias",
                                                "split_by_attribute_alias": "status",
                                                "split_with_color_gradients": false,
                                                "stacked": true
                                            }
                                        ],
                                        "axis_y": [
                                            {
                                                "attribute_alias": "app_alias",
                                                "hide_caption": true
                                            }
                                        ]
                                    }
                                ]
                            },
                            {
                                "caption": "Top long-runner scenarios",
                                "height": "33%",
                                "widgets": [
                                    {
                                        "widget_type": "DataTable",
                                        "object_alias": "axenox.BDT.run_scenario",
                                        "hide_header": true,
                                        "hide_footer": true,
                                        "paginator": {
                                            "count_all_rows": false,
                                            "page_size": 20
                                        },
                                        "paginate": false,
                                        "filters": [
                                            {
                                                "attribute_alias": "run_feature__run__finished_on",
                                                "comparator": ">=",
                                                "value": "=Today()",
                                                "hidden": true
                                            }
                                        ],
                                        "columns": [
                                            {
                                                "attribute_alias": "duration_ms"
                                            },
                                            {
                                                "attribute_alias": "name"
                                            },
                                            {
                                                "attribute_alias": "run_feature__name"
                                            }
                                        ],
                                        "sorters": [
                                            {
                                                "attribute_alias": "duration_ms",
                                                "direction": "desc"
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    }
}